<div class="main user-show">
  <div class="container">
    <div class="user">
        <img src="<%= "/user_images/#{@user.image_name}"%>">
      <!-- 以下で、ユーザー名を出力してください -->
      <h2><%= @user.name %></h2>
      <!-- 以下で、メールアドレスを出力してください -->
      <p><%= @user.email %></p>
      <p><%= @user.location %></p>
    </div>
  </div>
</div>